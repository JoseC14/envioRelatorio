  object sqlRelatorio: TFDQuery
    Active = True
    Connection = Conexao.Conexao
    SQL.Strings = (
      'SELECT tb_historico.id,tb_historico.cidade,tb_historico.orgao,'
      
        'tb_historico.tipo,tb_historico.dataenvio,tb_user.usuario, tb_use' +
        'r.id FROM tb_historico'
      
        'INNER JOIN tb_user ON tb_historico.idusuario = tb_user.id  WHERE' +
        ' dataenvio '
      'BETWEEN :datainicio  AND :datafim')
    Left = 104
    Top = 368
    ParamData = <
      item
        Name = 'DATAINICIO'
        DataType = ftString
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'DATAFIM'
        DataType = ftString
        ParamType = ptInput
        Value = Null
      end>
    object sqlRelatorioid: TIntegerField
      FieldName = 'id'
      Origin = 'id'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
    end
